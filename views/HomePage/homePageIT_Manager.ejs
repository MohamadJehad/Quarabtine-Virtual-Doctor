  <main id="main">
 <!-- ======= IT_Manager Table Section ======= -->
 <section id="IT_Manager" class="IT_Manager">
  <div class="container">

    <div class="section-title">
      
      <span>IT_Managers</span>
      <h2>IT_Managers</h2>
    </div>

    <style>
      .tableDoctor {
        text-align: center;
        background: #012B39;
        border-collapse: collapse;
        width: 100%;
        max-width: 1300px;
        margin: 0 auto;
        display: table;
      }
    </style>
    <table class="tableDoctor">
      <thead>
        <tr>
          <th>ID
            <th>Name
            <th>Mobile
            <th>Username 
            <th>Password
            <th>Gender
            <th>Remove IT_Manager
      </thead>
      <tbody>
       
            <% for (var i = 0; i < IT_Manager.length; i++) { %>
                <tr>
              <td name="id"><%= IT_Manager[i].ID %></td>
              <td><%= IT_Manager[i].FName %></td>
              <td><%= IT_Manager[i].mobile %></td>
              <td><%= IT_Manager[i].username %></td>
              <td><%= IT_Manager[i].password %></td>
              <td><%= IT_Manager[i].Gender %></td>
              <td>
                <form action="/removeIT_Manager" method="post">
                  <input type="hidden" name="IT_ManagerID" value="<%= IT_Manager[i].ID %>">
                  <input type="submit" value="Remove"style="background-color:#960f05; border: none; color: white;padding: 6px 6px;text-align: center; ;border-radius: 10px;">
                </form>
              </td>
                </tr>
            <% } %>
            <tr style="border-top: 1px solid white;">
          <td colspan="15"><a href="..\addIT_Manager" target="_blank">Add New IT_Manager</a>
          </tr>
          </tbody>
    </table>
 
    </div>

  </div>
</section><!-- End IT_Manager Table Section -->  

    <!-- ======= Doctor Table Section ======= -->
    <section id="Doctor" class="services">
      <div class="container">

        <div class="section-title">
          
          <span>Doctors</span>
          <h2>Doctors</h2>
        </div>

        <style>
          .tableDoctor {
            text-align: center;
            background: #012B39;
            border-radius: 0.25em;
            border-collapse: collapse;
            width: 100%;
            max-width: 1300px;
            
            margin: 0 auto;
            display: table;
          }
        </style>
        <table class="tableDoctor">
          <thead>
            <tr>
              <th>ID
                <th>Name
                <th>City
                <th>Street
                <th>BuildingNo
                <th>Mobile
                <th>Username 
                <th>Password
                <th>Gender
                <th>Specialization
                <th>Profile
          </thead>
          <tbody>
           
                <% for (var i = 0; i < doctors.length; i++) { %>
                    <tr>
                  <td name="id"><%= doctors[i].ID %></td>
                  <td><%= doctors[i].FName %></td>
                  <td><%= doctors[i].city %></td>
                  <td><%= doctors[i].street %></td>
                  <td><%= doctors[i].buildingNo %></td>
                  <td><%= doctors[i].mobile %></td>
                  <td><%= doctors[i].username %></td>
                  <td><%= doctors[i].password %></td>
                  <td><%= doctors[i].gender %></td>
                  <td><%= doctors[i].specialization %></td>
                  <td>
                    <form action="/DoctorProfile" method="post">
                      <input type="hidden" name="doctorID" value="<%= doctors[i].ID %>">
                      <input type="submit" value="Click Here"style="background-color: #4CAF50; border: none; color: white;padding: 6px 6px;text-align: center; ;border-radius: 10px;">
                    </form>
                  </td>
                    </tr>
                <% } %>
                <tr style="border-top: 1px solid white;">
              <td colspan="15"><a href="..\adddoctor" target="_blank">Add New Doctor</a>
              </tr>
              </tbody>
        </table>
     
        </div>

      </div>
    </section><!-- End Doctor Table Section -->  

    <div>
      <br>
    </div>
    
    

    <!-- ======= patient table Section ======= -->
    <section id="Patient" class="services">
      <div class="container">
    

        <div class="section-title">
          <span>Patients</span>
          <h2>Patients</h2>
        </div>

        <style>
          .tablePatient {
            text-align: center;
            background: #012B39;
            border-radius: 0.25em;
            border-collapse: collapse;
            width: 100%;
            max-width: 1300px;
            
            margin: 0 auto;
            display: table;
          }
        </style>
        <table class="tablePatient" id="table" >
          <thead>
            <tr>
              <th>ID
                <th>Name
                <th>City
                <th>Street
                <th>BuildingNo
                <th>Mobile
                <th>Birth-Date 
                <th>Gender
                <th>Weight
                <th>Assigned-Doctor
                <th>Room
                <th>Profile
            </thead>
            <tbody>
             
              <% for (var i = 0; i < patients.length; i++) { %>
                  <tr>
                    <td><%= patients[i].ID %></td>
                    <td><%= patients[i].FName %></td>
                    <td><%= patients[i].city %></td>
                    <td><%= patients[i].street %></td>
                    <td><%= patients[i].buildingNo %></td>
                    <td><%= patients[i].mobile %></td>
                    <td><%= patients[i].birthDate %></td>
                    <td><%= patients[i].gender %></td>
                    <td><%= patients[i].weight %></td>
                    <td><%= patients[i].assigned_doctor %></td>
                    <td><%= patients[i].room_id %></td>
                    <td>
                      <form action="/PatientProfileForIT_Manager" method="post">
                        <input type="hidden" name="patientID" value="<%= patients[i].ID %>">
                        <input type="submit" value="Click Here"style="background-color: #4CAF50; border: none; color: white;padding: 6px 6px;text-align: center; ;border-radius: 10px;">
                      </form>
                    </td>
                    
                    
                  </tr>
                  <% } %>
                  <tr style="border-top: 1px solid white;">
              <td colspan="16"><a href="..\addpatient" target="_blank">Add New Patient</a>
              </tr>
          </tbody>
        </table>
        </div>
    </section><!-- End Services Section -->


