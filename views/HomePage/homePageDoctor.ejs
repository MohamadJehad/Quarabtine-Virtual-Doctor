    <section id="Patients" class="services">
      <div class="container">

        <div class="section-title">
          <span>Patients</span>
          <h2>Patients</h2>
        </div>

        <style>
          .tablePatient {
            text-align: center;
            background: #012B39;
            border-radius: 0.25cm;

            border-collapse: none;
            width: 100%;
            max-width: 1200px;
            
            margin: 0 auto;
            display: table;
          }
        </style>
        <table class="tablePatient" id="table" >
          <thead>
           <tr>
              <th>ID
              <th>Name
              <th>city
              <th>street
              <th>buildingNo
              <th>Mobile
              <th>Birth-Date 
              <th>Gender
              <th>Weight
              <th>Profile
          </thead>
          <tbody>
            
            <% for (var i = 0; i < patients.length; i++) { %>
                <tr>
                  <td><%= patients[i].ID %></td>
                  <td><%= patients[i].FName %></td>
                  <td><%= patients[i].city %></td>
                  <td><%= patients[i].street %></td>
                  <td><%= patients[i].buildingNo %></td>
                  <td><%= patients[i].mobile %></td>
                  <td><%= patients[i].birthDate %></td>
                  <td><%= patients[i].gender %></td>
                  <td><%= patients[i].weight %></td>
                  <td>
                    <form action="/PatientProfileForDoctor" method="post">
                      <input type="hidden" name="patientID" value="<%= patients[i].ID %>">
                      <input type="submit" value="Click Here" style="background-color: #4CAF50;color: white; border: 0px solid transparent;;padding: 6px 6px;text-align: center;border-radius: 10px;">
                    </form>
                  </td>
                </tr>
                <% } %>
          </tbody>
        </table>
        </div>

    </section><!-- End Services Section -->

<br><br>
