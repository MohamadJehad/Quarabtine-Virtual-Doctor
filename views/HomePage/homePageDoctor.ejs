            <button class="button down" onclick="sendDirection('down')">Down</button>  
            <button class="button left" onclick="sendDirection('left')">Left</button>  
            <button class="button right" onclick="sendDirection('right')">Right</button>  
        </div>
            <style type="text/css">
              .button-container {
                  display: flex;
                  justify-content: center;
                  align-items: center;
                  height: 50vh;
              }
              
              .button {
